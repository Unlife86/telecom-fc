{
    "name": "Football Club Telecom",
    "description": "App Yii2 FC Telecom",
    "keywords": ["yii2", "framework", "football team", "telecom"],
    "homepage": "http://www.telecom-fc.ru",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "^2.0",
        "kartik-v/yii2-widget-datetimepicker": "dev-master",
        "wbraganca/yii2-dynamicform": "*",
        "kartik-v/yii2-grid": "^3.1",
        "kartik-v/yii2-editable": "^1.7",
        "kartik-v/yii2-mpdf": "^1.0",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "kartik-v/yii2-checkbox-x": "^1.0",
        "kartik-v/yii2-widget-activeform": "^1.4",
        "kartik-v/yii2-builder": "^1.6",
        "sersid/yii2-font-awesome-asset": "^1.0",
        "yiisoft/yii2": "^2.0.13",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "kartik-v/yii2-markdown": "^1.3",
        "league/html-to-markdown": "^4.6"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
